{
  "name": "y-ace",
  "version": "0.1.4",
  "description": "Ace binding for Yjs",
  "main": "./dist/y-ace.cjs",
  "module": "./src/y-ace.js",
  "types": "./dist/src/y-ace.d.ts",
  "sideEffects": false,
  "funding": {
    "type": "GitHub Sponsors ‚ù§",
    "url": "https://github.com/sponsors/dmonad"
  },
  "scripts": {
    "start": "npm run demo",
    "demo": "concurrently 'live-server --port=3443 --entry-file=demo/index.html' 'npm run watch'",
    "dist": "rm -rf dist && rollup -c",
    "test": "rollup -c && node test/test.cjs",
    "lint": "standard && tsc",
    "watch": "rollup -wc",
    "debug": "concurrently 'live-server --port=3443 --entry-file=test.html' 'npm run watch'",
    "preversion": "npm run dist && npm run lint && npm run test && test -e ./dist/src/y-ace.d.ts && test -e ./dist/y-ace.cjs && test -e ./dist/y-ace.cjs.map"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjs/y-ace.git"
  },
  "keywords": [
    "Yjs"
  ],
  "author": "Kevin Jahns <kevin.jahns@protonmail.com>",
  "contributors": [
  	"Ted Davis"
  ],
  "license": "MIT",
  "standard": {
    "ignore": [
      "/dist",
      "/node_modules",
      "/docs"
    ]
  },
  "bugs": {
    "url": "https://github.com/yjs/y-ace/issues"
  },
  "homepage": "https://github.com/yjs/y-ace#readme",
  "dependencies": {
    "lib0": "^0.2.31",
    "y-protocols": "^1.0.0"
  },
  "peerDependencies": {
    "yjs": "^13.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "ace-builds": "^1.4.12",
    "concurrently": "^4.1.2",
    "jsdom": "^15.2.1",
    "live-server": "^1.2.1",
    "rollup": "^2.36.1",
    "standard": "^12.0.1",
    "y-webrtc": "^10.1.8",
    "y-websocket": "^1.3.11",
    "yjs": "^13.4.11"
  }
}
